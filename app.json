{
  "expo": {
    "name": "RNGCExpo",
    "slug": "test-dev",
    "owner": "denispavlovic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      ["@config-plugins/react-native-google-cast"]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.denispavlovic.RNGCExpo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.denispavlovic.RNGCExpo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "89991893-dd24-462f-a41b-1c5ed3ab24eb"
      }
    }
  }
}
